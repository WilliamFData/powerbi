    "SELECT\n",
    "  d AS DATE_ID\n",
    "FROM\n",
    "  explode(\n",
    "    sequence(\n",
    "      to_date('2000-01-01'),\n",
    "      date_add(to_date('2000-01-01'), 18628 - 1),\n",
    "      interval 1 day\n",
    "    )\n",
    "  ) AS t(d);"
